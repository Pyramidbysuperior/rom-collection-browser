<window>
	<allowoverlay>no</allowoverlay>
	<defaultcontrol always="false">3000</defaultcontrol>
	<coordinates>
	        <system>1</system>
        	<posx>0</posx>
        	<posy>0</posy>
	</coordinates>
	<controls>
		<control type="image">
		    <description>background image</description>
		    <posx>0</posx>
		    <posy>0</posy>
		    <width>720</width>
		    <height>580</height>
		    <texture>background.png</texture>
		</control>
		<control type="label">			
			<posx>50</posx>
			<posy>20</posy>
		    <textcolor>DDFFFFFF</textcolor>
		    <label>Game:</label>
		</control>
		<control type="label" id="6000">
			<description>Game value</description>
			<posx>150</posx>
			<posy>20</posy>
		    <textcolor>DDFFFFFF</textcolor>
		</control>
		<control type="label">
			<posx>50</posx>
			<posy>50</posy>
		    <textcolor>DDFFFFFF</textcolor>
		    <label>Genre:</label>
		</control>
		<control type="label" id="6100">
			<description>Genre value</description>
			<posx>150</posx>
			<posy>50</posy>
		    <textcolor>DDFFFFFF</textcolor>
		</control>
		<control type="label">
			<posx>50</posx>
			<posy>80</posy>
		    <textcolor>DDFFFFFF</textcolor>
		    <label>Year:</label>
		</control>
		<control type="label" id="6200">
			<description>Year value</description>
			<posx>150</posx>
			<posy>80</posy>
		    <textcolor>DDFFFFFF</textcolor>
		</control>
		<control type="label">
			<posx>50</posx>
			<posy>110</posy>
		    <textcolor>DDFFFFFF</textcolor>
		    <label>Publisher:</label>
		</control>
		<control type="label" id="6300">
			<description>Publisher value</description>
			<posx>150</posx>
			<posy>110</posy>
		    <textcolor>DDFFFFFF</textcolor>
		</control>		
		<control type="image" id="2000">
		    	<description>console image</description>
		    	<posx>100</posx>
		    	<posy>340</posy>
		    	<width>200</width>
		    	<height>150</height>			
			<texture></texture>
		</control>
		<!-- Console -->
		<control type="textbox" id="2100">
			<description>Consoles</description>
			<posx>350</posx>
			<posy>340</posy>
			<width>275</width>
		    <height>150</height>			
			<pagecontrol>2200</pagecontrol>			
			<textcolor>DDFFFFFF</textcolor>			
			<autoscroll delay="3000" time="1000" repeat="10000">!Control.HasFocus(2200)</autoscroll>
		</control>
		<control type="scrollbar" id="2200">
			<description>ConsoleDesc Scrollbar</description>
			<posx>630</posx>
			<posy>345</posy>
			<width>5</width>
			<height>150</height>
			<visible>Control.IsVisible(2100)</visible>
			<texturesliderbackground>scrollbar-bg.png</texturesliderbackground>
			<texturesliderbar>scrollbar-bar.png</texturesliderbar>
			<texturesliderbarfocus>scrollbar-barFocus.png</texturesliderbarfocus>
			<textureslidernib>scrollbar-nib.png</textureslidernib>
			<textureslidernibfocus>scrollbar-nibFocus.png</textureslidernibfocus>
			<pulseonselect></pulseonselect>
			<orientation>vertical</orientation>
			<showonepage>false</showonepage>
			<onup>2200</onup>
			<ondown>2200</ondown>
			<onleft>3200</onleft>
			<onright>800</onright>
		</control>
		<control type="button" id="3000"> 
			<description>Import Settings</description>
			<posx>50</posx>
			<posy>525</posy>
			<width>100</width>
			<height>30</height>
			<visible>true</visible>
			<colordiffuse>DDFFFFFF</colordiffuse>
			<texturefocus>btnFocus.png</texturefocus>
			<texturenofocus>btn.png</texturenofocus>
			<label>Import Settings</label>
			<!--font12-->
			<textcolor>DDFFFFFF</textcolor>
			<disabledcolor>80FFFFFF</disabledcolor>
			<align>center</align>
			<aligny>center</aligny>
			<textoffsetx></textoffsetx>
			<textoffsety></textoffsety>
			<pulseonselect></pulseonselect>
			<onup>1500</onup>
			<ondown>500</ondown>
			<onleft>500</onleft>
			<onright>3100</onright>
		</control>
		<control type="button" id="3100"> 
			<description>Update DB</description>
			<posx>175</posx>
			<posy>525</posy>
			<width>100</width>
			<height>30</height>
			<visible>true</visible>
			<colordiffuse>DDFFFFFF</colordiffuse>
			<texturefocus>btnFocus.png</texturefocus>
			<texturenofocus>btn.png</texturenofocus>
			<label>Update DB</label>
			<!--font12-->
			<textcolor>DDFFFFFF</textcolor>
			<disabledcolor>80FFFFFF</disabledcolor>
			<align>center</align>
			<aligny>center</aligny>
			<textoffsetx></textoffsetx>
			<textoffsety></textoffsety>
			<pulseonselect></pulseonselect>
			<onup>1500</onup>
			<ondown>500</ondown>
			<onleft>3000</onleft>
			<onright>3200</onright>
		</control>
		<control type="button" id="3200"> 
			<description>Change View</description>
			<posx>300</posx>
			<posy>525</posy>
			<width>100</width>
			<height>30</height>
			<visible>true</visible>
			<colordiffuse>DDFFFFFF</colordiffuse>
			<texturefocus>btnFocus.png</texturefocus>
			<texturenofocus>btn.png</texturenofocus>
			<label>Change View</label>
			<!--font12-->
			<textcolor>DDFFFFFF</textcolor>
			<disabledcolor>80FFFFFF</disabledcolor>
			<align>center</align>
			<aligny>center</aligny>
			<textoffsetx></textoffsetx>
			<textoffsety></textoffsety>
			<pulseonselect></pulseonselect>
			<onup>1500</onup>
			<ondown>500</ondown>
			<onleft>3100</onleft>
			<onright>2200</onright>
		</control>
		<control type="label" id="4000">
			<description>Notes</description>
			<posx>700</posx>
			<posy>560</posy>
			<width>680</width>
			<height>10</height>
			<align>right</align>
			<scroll>true</scroll>
		    <textcolor>DDFFFFFF</textcolor>
		    <label></label>
			<font>font10</font>
		</control>
	</controls>
</window>
