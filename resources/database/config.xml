			<ingamevidPath>E:\Emulatoren\data\Amiga\Collection V1\ingameVideo\%GAME%.wmv</ingamevidPath>
			<trailerPath>E:\Emulatoren\data\Amiga\Collection V1\trailer\%GAME%.wmv</trailerPath>
